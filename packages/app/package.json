{
  "name": "@ootc/yah",
  "version": "0.0.4",
  "type": "module",
  "bin": {
    "yah": "bin/yah.js"
  },
  "files": [
    "README.md",
    "bin",
    "dist",
    "data",
    "runtime"
  ],
  "scripts": {
    "dev": "tsx watch src/cli.ts start",
    "dev:cli": "node --import tsx src/cli.ts",
    "start": "node --import tsx src/cli.ts start",
    "build": "tsc -p tsconfig.json",
    "smoke:db": "node --import tsx tests/db-smoke.ts",
    "build:frontend": "pnpm -C ../frontend build",
    "build:runtime": "node scripts/bundle-runtime.mjs",
    "verify:runtime": "node scripts/verify-runtime.mjs",
    "prepublishOnly": "pnpm run build && pnpm run smoke:db && pnpm run build:frontend && pnpm run build:runtime && pnpm run verify:runtime"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "openai": "^6.21.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
